{
  "name": "front-customer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.2.5",
    "@mui/lab": "5.0.0-alpha.63",
    "@mui/material": "5.2.7",
    "@mui/styles": "5.2.3",
    "@react-google-maps/api": "^2.18.1",
    "@sentry/react": "6.13.3",
    "@sentry/tracing": "6.13.3",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-redux": "7.1.22",
    "@types/redux-saga": "0.10.5",
    "axios": "0.21.3",
    "cross-env": "7.0.3",
    "dayjs": "1.10.7",
    "email-validator": "2.0.4",
    "env-cmd": "10.1.0",
    "formik": "2.2.9",
    "hex-to-rgba": "^2.0.1",
    "i18next": "21.8.2",
    "i18next-http-backend": "^1.3.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.41",
    "query-string": "6.14.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-autocomplete": "^2.4.3",
    "react-google-recaptcha-v3": "1.9.5",
    "react-hook-form": "7.12.2",
    "react-i18next": "11.16.9",
    "react-input-mask": "^2.0.4",
    "react-multi-carousel": "2.6.5",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "7.2.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.2.1",
    "react-scripts": "5.0.1",
    "react-text-mask": "^5.5.0",
    "redux": "4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "1.1.3",
    "rut.js": "1.0.2",
    "text-mask-addons": "^3.8.0",
    "uuid": "9.0.0",
    "web-vitals": "1.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/__stories__/",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "jest-junit",
      "text",
      "cobertura",
      "json",
      "lcov",
      "clover"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " › ",
    "usePathForSuiteName": "true"
  },
  "@casualbot/jest-sonar-reporter": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "outputName": "jest-report.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " › ",
    "usePathForSuiteName": "true",
    "relativePaths": "true"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:local": "cross-env NODE_ENV=local env-cmd -f env/env.local react-scripts start",
    "start:dev": "cross-env NODE_ENV=development env-cmd -f env/env.dev react-scripts start",
    "start:qa": "cross-env NODE_ENV=qa env-cmd -f env/env.qa react-scripts start",
    "start:uat": "cross-env NODE_ENV=uat env-cmd -f env/env.uat react-scripts start",
    "start:tuten": "cross-env NODE_ENV=demo env-cmd -f env/env.demo react-scripts start",
    "start:learn": "cross-env NODE_ENV=learn env-cmd -f env/env.learn react-scripts start",
    "start:labs": "cross-env NODE_ENV=labs env-cmd -f env/env.labs react-scripts start",
    "start:master": "cross-env NODE_ENV=production env-cmd -f env/env.master react-scripts start",
    "build:local": "cross-env NODE_ENV=local env-cmd -f env/env.local react-scripts build",
    "build:dev": "cross-env NODE_ENV=development env-cmd -f env/env.dev react-scripts build",
    "build:qa": "cross-env NODE_ENV=qa env-cmd -f env/env.qa react-scripts build",
    "build:uat": "cross-env NODE_ENV=uat env-cmd -f env/env.uat react-scripts build",
    "build:tuten": "cross-env NODE_ENV=demo env-cmd -f env/env.demo react-scripts build",
    "build:learn": "cross-env NODE_ENV=learn env-cmd -f env/env.learn react-scripts build",
    "build:labs": "cross-env NODE_ENV=labs env-cmd -f env/env.labs react-scripts build",
    "build:master": "cross-env NODE_ENV=production env-cmd -f env/env.master react-scripts build",
    "citest": "CI=true react-scripts test --testResultsProcessor=jest-junit & CI=true react-scripts test --coverage --testResultsProcessor=@casualbot/jest-sonar-reporter --coverageReporters=cobertura --coverageReporters=lcov --coverageReporters=html",
    "lint:fix": "eslint --fix --ext .js,.jsx,.tsx .",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@casualbot/jest-sonar-reporter": "^2.2.5",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.23",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-text-mask": "^5.4.13",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "classnames": "2.3.1",
    "cypress": "^9.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "26.6.0",
    "prettier": "2.5.1",
    "redux-logger": "3.0.6",
    "typescript": "4.4.2",
    "xss": "1.0.8"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
